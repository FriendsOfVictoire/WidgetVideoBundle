<iframe frameborder="0"
        {% if null != width %}
            width="{{ width }}"
        {% endif %}
        {% if null != height %}
            height="{{ height }}"
        {% endif %}
        src="//www.dailymotion.com/embed/video/{{ code }}?autoplay={{ autoPlay ? 1 : 0 }}&controls={{ controls ? 1 : 0 }}"
        allowfullscreen>
</iframe>